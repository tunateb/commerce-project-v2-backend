{
  "kind": "collectionType",
  "collectionName": "purchase",
  "info": {
    "name": "purchase"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "delivered",
        "ordered"
      ],
      "default": "ordered"
    },
    "user": {
      "via": "purchases",
      "plugin": "users-permissions",
      "model": "user"
    },
    "address": {
      "model": "address"
    },
    "cart": {
      "model": "cart"
    },
    "cardNumber": {
      "type": "string",
      "required": true
    },
    "cardName": {
      "type": "string",
      "required": true
    },
    "cvc": {
      "type": "string",
      "required": true
    },
    "expDate": {
      "type": "string",
      "required": true
    }
  }
}
